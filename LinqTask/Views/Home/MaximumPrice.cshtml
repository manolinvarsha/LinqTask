@model IEnumerable<LinqTask.Models.Monthwisemaxprice>

@{
    ViewBag.Title = "MaximumPrice";
}

<h2>MaximumPrice</h2>

<!DOCTYPE html>
<html>
<head>

</head>
<body>
    <div class="container-fluid py-4">
        <h4 class="text-center text-uppercase">
            Maximum order price order and month wise
        </h4>
        <table class="table table-striped table-bordered">

            <thead class="bg-dark text-white">
                <tr>
                    <th>Date</th>
                    <th>POID </th>
                    <th>Price</th>

                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
    <tr>
        <td>@item.Date.ToString("MMM/yy")</td>
        <td>@item.POID.POID </td>
        <td>@item.Price</td>
    </tr>}
            </tbody>
        </table>
    </div>
    <div>
        <a href="Index">Back</a>

    </div>
</body>
</html>