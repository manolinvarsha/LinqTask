@model IEnumerable<LinqTask.Models.ListOfPurchaseOrders>

@{ ViewBag.Title = "ProductNameAsc"; }


<!DOCTYPE html>
<html>
<head>

</head>
<body>
    <div class="container-fluid py-4">
        <h4 class="text-center text-uppercase">Product Name in Ascending Order</h4>
        <table class="table table-striped table-bordered">

            <thead class="bg-dark text-white">
                <tr>
                    <th>Product Name</th>
                    
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
    <tr>
        <td>@item.product.ProductName</td>
 
    </tr>
}
            </tbody>
        </table>
    </div>
    <div>
        <a href="Index">Back</a>

    </div>
</body>
</html>
